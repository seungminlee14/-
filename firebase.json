{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      { "source": "/community/**", "destination": "/community.html" }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
